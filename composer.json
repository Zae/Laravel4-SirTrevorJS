{
  "name": "caouecs/sirtrevorjs",
  "description": "Sir Trevor JS in Laravel project",
  "keywords": ["laravel", "sir trevor js", "sir-trevor-js", "lpm"],
  "license": "MIT",
  "authors": [
    {
      "name": "caouecs",
      "email": "caouecs@caouecs.net"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "laravel/framework": "^5.0 | ^6.0 | ^7.0",
    "erusev/parsedown-extra": "~0.2",
    "laravelcollective/html": "^5.0 | ^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0.0 | ^9.0.0"
  },
  "autoload": {
    "psr-4": {
      "Caouecs\\": "src/Caouecs"
    }
  },
  "autoload-dev": {
    "classmap": [
      "test"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Caouecs\\Sirtrevorjs\\SirtrevorjsServiceProvider"
      ]
    }
  }
}
